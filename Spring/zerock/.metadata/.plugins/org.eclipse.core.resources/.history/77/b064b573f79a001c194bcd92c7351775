package org.zerock.domain;

import lombok.Getter;
import lombok.Setter;
import lombok.ToString;

public class Criteria {
    
    private int pageNum;
    private int amount;

    @Getter
    @Setter
    @ToString
    public Criteria() {
        this(1,10);
    }

    
    
    public Criteria(int pageNum, int amount) {
        super();
        this.pageNum = pageNum;
        this.amount = amount;
    }


}
